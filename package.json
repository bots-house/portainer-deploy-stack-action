{
  "name": "portainer-stack-deploy-action",
  "version": "0.0.0",
  "private": true,
  "description": "Action for deploy Docker Stack via Portainer API",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bots-house/portainer-deploy-stack-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "docker",
    "portainer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "axios": "^0.21.1",
    "js-yaml": "^3.14.1",
    "ts-custom-error": "^3.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.14.35",
    "@typescript-eslint/parser": "^4.11.1",
    "@vercel/ncc": "^0.26.1",
    "eslint": "^7.16.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "prettier": "2.2.1",
    "typescript": "^4.0.2"
  }
}
